{
    "version": 2, 
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node",
            "outputDirectory": "public"

        }
],
    "routes": [
        {
            "src": "/{.*}",
            "dest": "public/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH"
            ]
        }
    ]
}